<channel>
  <title><%= channel.title %></title>
  <link><%= channel.baseUrl %></link>
  <atom:link href="<%= channel.baseUrl %>/feeds/rss.xml" rel="self" type="application/rss+xml" />
  <description><%= channel.description %></description>

  <language><%= channel.language %></language>
  <copyright><%= channel.copyright %></copyright>
  <lastBuildDate><%= channel.lastBuildDate %></lastBuildDate>
  <docs>http://www.rssboard.org/rss-specification</docs>
  <creativeCommons:license><%= channel.license %></creativeCommons:license>
  <sy:updatePeriod>weekly</sy:updatePeriod>
  <sy:updateFrequency>1</sy:updateFrequency>

  <%- articles %>

</channel>
